{
  "printWidth": 80,
  "endOfLine": "auto",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": false,
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "parser": "typescript"
}
